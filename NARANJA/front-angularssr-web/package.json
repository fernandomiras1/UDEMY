{
  "name": "webapp",
  "version": "1.2.3",
  "config": {
    "build": "develop"
  },
  "scripts": {
    "start:stubs": "concurrently \"ng serve -c stubs --host 0.0.0.0 \" \"npm:stubs\"",
    "start:proxy": "ng serve -c proxy --proxy-config proxy.conf.json",
    "start:ssr": "node dist/server",
    "build": "npm-run-all clean build:front:**",
    "build:dev": "npm-run-all clean build:front:** --webapp:build=develop",
    "build:tst": "npm-run-all clean build:front:** --webapp:build=testing",
    "build:stg": "npm-run-all clean build:front:** --webapp:build=staging",
    "build:prd": "npm-run-all clean build:front:** --webapp:build=prodblue",
    "build:front:ng": "ng build --configuration=${npm_package_config_build} && ng run webapp:server:${npm_package_config_build}",
    "build:front:pwa": "node workbox-build-inject.js",
    "build:front:copy": "cp src/robots.txt dist/browser",
    "build:front:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test --browsers=Chrome",
    "test:build": "rimraf ./coverage && ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "lint": "npm-run-all -p lint:*",
    "lint:app": "tslint \"src/**/*.ts\" --project tsconfig.json --fix -t verbose",
    "lint:scss": "stylelint \"src/**/*.scss\" --syntax scss",
    "clean": "rimraf dist",
    "report": "ng build -c prodblue --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "release": "standard-version -t ''",
    "release:major": "npm run release -- --release-as major",
    "release:minor": "npm run release -- --release-as minor",
    "release:patch": "npm run release -- --release-as patch",
    "release:beta": "npm run release -- --prerelease beta",
    "generate": "plop",
    "stubs": "stubby --data stubs/stubs-config.yaml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@auth0/auth0-spa-js": "^1.6.0",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/router-store": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@nguniversal/common": "^7.1.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^0.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bundle-loader": "^0.5.6",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "domino": "^2.1.3",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "lazysizes": "^5.1.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26",
    "zumo": "^2.8.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/schematics": "^7.3.8",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@ngrx/schematics": "^7.4.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^5.0.2",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.32",
    "lighthouse": "4.0.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.5.3",
    "protractor": "~5.4.0",
    "puppeteer": "^1.16.0",
    "rimraf": "^2.6.3",
    "standard-version": "^7.0.0",
    "stubby": "^4.1.1",
    "stylelint-config-suitcss": "^14.0.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.0",
    "workbox-build": "^4.3.1"
  },
  "standard-version": {
    "skip": {
      "tag": true,
      "commit": false
    },
    "scripts": {
      "postchangelog": "bash fixChangelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:build"
    }
  }
}
